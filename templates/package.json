{
  "name": "<%= name %>",
  "description": "<%= description %>",
  "version": "0.1.0",
  "homepage": "https://github.com/<%= github_user_name %>/<%= name %>",
  "author": {
    "name": "<%= author_name %>",
    "email": "<%= author_email %>"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/<%= github_user_name %>/<%= name %>.git"
  },
  "bugs": {
    "url": "https://github.com/<%= github_user_name %>/<%= name %>/issues"
  },
  "license": "<%= license  %>",
  "main": "dist/<%= name %>",
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    <% if (typescript) { %>
    "postinstall": "typings install --overwrite --save",
    <% } %>
    <% if (gulp) { %>
    "test:gulp": "./node_modules/gulp/bin/gulp.js",
    <% } %>
    <% if (karma) { %>
    "test:karma": "karma start",
    <% } %>
    <% if (webpack) { %>
    "test:webpack": "mocha-webpack --webpack-config webpack.config-test.js \".tmp/test/**/*.test.js\"",
    <% } %>
    "cover": "nyc ./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha .tmp/test/src"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "del": "^2.2.2",
    "glob": "^7.1.0",
    <% if (babel) { %>
    "babel-cli": "^6.16.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2017": "^6.16.0"
    <% } %>
    <% if (nyc) { %>
    "nyc": "^8.4.0",
    <% } %>
    <% if (gulp) { %>
    "gulp": "^3.9.1",
    "gulp-changed": "^1.3.2",
    "gulp-espower": "^1.0.2",
    "gulp-jshint": "^2.0.1",
    "gulp-load-plugins": "^1.3.0",
    "gulp-plumber": "^1.1.0",
    "gulp-replace": "^0.5.4",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-spawn-mocha": "^3.1.0",
    <%   if (typescript) { %>
    "gulp-tslint": "^3.0.1",
    "gulp-typescript": "^3.0.2",
    <%   } %>
    <% } %>
    <% if (istanbul) { %>
    "istanbul": "^0.4.5",
    <% } %>
    "jshint": "^2.0.1",
    <% if (karma) { %>
    "karma": "^1.3.0",
    "karma-chai": "^0.1.0",
    "karma-mocha": "^1.2.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sinon": "^1.0.5",
    "karma-typescript-preprocessor2": "^1.2.1",
    "karma-webpack": "^1.8.0",
    <% } %>
    "merge-stream": "^1.0.0",
    "mocha": "^3.1.0",
    <% if (webpack) { %>
    "mocha-webpack": "^0.6.0",
    <% } %>
    <% if (sinon) { %>
    "sinon": "^1.17.6",
    <% } %>
    "source-map-support": "^0.4.3",
    <% if (typescript) { %>
    "ts-loader": "^0.8.2",
    "typescript": "^2.0.3",
    <% } %>
    <% if (webpack) { %>
    <%   if (webpackServer) { %>
    "webpack-dev-server": "^1.16.2",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.6.0",
    <%   } %>
    "webpack": "^1.13.2",
    "webpack-node-externals": "^1.5.4",
    "webpack-merge": "^0.15.0"
    <% } %>
    "node-inspector": "^0.12.8",
    "open": "^0.0.5",
    "phantomjs-prebuilt": "^2.1.12",
    "run-sequence": "^1.2.2"
  },
  "keywords": [],
  "dependencies": {},
  "typescript": {
    "definition": "dist/<%= name %>.d.ts"
  }
}
